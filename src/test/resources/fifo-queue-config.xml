<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:fifo-queue="http://www.mulesoft.org/schema/mule/fifo-queue"
      xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
        http://www.mulesoft.org/schema/mule/fifo-queue http://www.mulesoft.org/schema/mule/fifo-queue/1.0/mule-fifo-queue.xsd">

    <fifo-queue:config name="fifoQueueConnector" objectStore-ref="_defaultUserObjectStore" />

    <flow name="putFlow">
        <fifo-queue:put queue="queue1"/>
        <fifo-queue:put queue="queue2"/>
        <fifo-queue:put queue="queue3"/>
        <fifo-queue:put queue="queue4"/>
        <fifo-queue:put queue="queue5"/>
    </flow>
    
    <flow name="sizeFlow">
        <fifo-queue:size queue="queue1"/>
    </flow>
    
    <flow name="peakFlow">
        <fifo-queue:peak queue="queue1"/>
    </flow>
    
    <flow name="takeFlow">
        <fifo-queue:take queue="queue1"/>
    </flow>
    
    <flow name="markErrorFlow">
        <fifo-queue:mark-error queue="queue1"/>
    </flow>
    
    <flow name="resolveErrorFlow">
        <fifo-queue:resolve-error queue="queue1"/>
    </flow>
    
    <flow name="statusFlow">
        <fifo-queue:status queue="queue1"/>
    </flow>
    
    <flow name="peakAllFlow">
        <fifo-queue:peak-all/>
    </flow>
    
    <flow name="takeAllFlow">
        <fifo-queue:take-all/>
    </flow>
    
    <flow name="drainFlow">
        <fifo-queue:drain queue="queue1"/>
    </flow>
    
    <flow name="drainAllFlow">
        <fifo-queue:drain-all/>
    </flow>
    
    
</mule>
